<%= form_for @user do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name%><br>

  <%= f.label :tickets %><br>
  <%= f.text_field :tickets %><br>

  <%= f.label :height, "Height (in inches)"%><br>
  <%= f.text_field :height %><br>

  <%= f.label :nausea, "How nauseous are you (1-5)" %><br>
  <%= f.text_field :nausea %><br>

  <%= f.label :happiness, "How miserable are you (1-5)" %><br>
  <%= f.text_field :happiness %><br>

  <%= f.label :admin, "Are you a theme park owner?" %><br>
  <%= f.check_box :admin %><br>

  <%= f.submit %>
<% end %>